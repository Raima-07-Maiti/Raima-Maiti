#include <stdio.h>

int main()
{
   int year; 
   printf("\nEnter any year: ");
   scanf("%d", &year);
   if(year%100==0)
   {
       if(year%400==0)
       {
           printf("The entered year is a Leap Year.");
       }
       else
       {
           printf("The entered year is NOT a Leap Year.");
       }
   }
   else if(year%4==0)
   {
       printf("The entered year is a Leap Year.");
   }
   else
   {
       printf("The entered year is NOT a Leap Year.");

   }

    return 0;
}
