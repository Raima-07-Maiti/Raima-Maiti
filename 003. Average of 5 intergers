#include <stdio.h>

int main()
{
    int num1, num2, num3, num4, num5; 
    float average;
    printf("Input five intergers: ");
    scanf("%d %d %d %d %d", &num1, &num2, &num3, &num4, &num5);
    average= (float)(num1+num2+num3+num4+num5)/5;
    printf("The average of the five intergers is: %.2f", average);
    
    return 0;
}
