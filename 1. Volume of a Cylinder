#include <stdio.h>
#define PI 3.14 

int main()
{
    int radius, height; 
    float volume;
    printf("\nInput the radius of the cylinder: ");
    scanf("%d", &radius);
    printf("\nInput the height of the cylinder: ");
    scanf("%d", &height);
    volume=PI*radius*radius*height;
    printf("\nThe volume of the cylider is: %f", volume);

    return 0;
}
