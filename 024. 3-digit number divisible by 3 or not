#include <stdio.h>

int main()
{
    int number; 
    printf("\nEnter a 3-digit number: ");
    scanf("%d", &number);
    if(number%100>0 && number&100<10)
    {
        if(number%3==0)
        {
            printf("\nThe number is divisibe by 3.");
        }
        else
        {
            printf("\nThe number is NOT divisibe by 3."); 
        }
    }
    else
    {
        printf("\nThe entered number is not a 3-digit number.");
    }

    return 0;
}
