#include <stdio.h>

int main()
{
    int n, i, maximum, maximum2; 
    printf("\nEnter the number of elements to be stored in the array: ");
    scanf("%d", &n);
    int arr[n];
    
    printf("\nEnter the array elements: ");
    for(i=0; i<n; i++)
    {
        scanf("%d", &arr[i]);
    }
    if(arr[0]>arr[1])
    {
    maximum=arr[0];
    maximum2=arr[1];
    }
    else
    {
        maximum=arr[1];
        maximum2=arr[0];
    }

    for(i = 2; i <n; i++)
    {
        if(arr[i] > maximum)
        {
            maximum2 = maximum;
            maximum = arr[i];
        }
        else if(arr[i] > maximum2 && arr[i] < maximum)
        {
            maximum2 = arr[i];
        }
    }
    printf("\nThe maximum element in the array is: %d", maximum);
    printf("\nThe second maximum element in the array is: %d", maximum2);


    return 0;
}
