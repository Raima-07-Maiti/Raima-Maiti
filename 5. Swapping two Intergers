#include <stdio.h>

int main()
{
    int num1, num2, temp; 
    printf("\nInput the first number: ");
    scanf("%d", &num1);
    printf("\nInput the second number: ");
    scanf("%d", &num2); 
    
    temp= num1 + num2;
    num1= temp-num1; 
    num2= temp-num2;
    
    printf("\nAfter swapping, the first number is: %d", num1); 
    printf("\nAnd the second number is: %d", num2); 
    
    return 0;
}
