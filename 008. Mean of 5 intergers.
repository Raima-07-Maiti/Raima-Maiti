#include <stdio.h>

int main()
{
    int num1, num2, num3, num4, num5; 
    float mean;
    printf("Input five intergers: ");
    scanf("%d %d %d %d %d", &num1, &num2, &num3, &num4, &num5);
    mean= (float)(num1+num2+num3+num4+num5)/5.0f;
    printf("The mean of the five intergers is: %.2f", mean);
    
    return 0;
}
