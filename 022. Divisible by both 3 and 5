#include <stdio.h>

int main()
{
    int number; 
    printf("\nEnter any number: ");
    scanf("%d", &number);
    if(number%3==0 && number%5==0)
    {
        printf("\nThe entered number is divisible by both 3 and 5.");
    }
    else
    {
        printf("\nThe entered number is NOT divisible by both 3 and 5.");
  
    }
    return 0;
}
