#include <stdio.h>

int main()
{
    int i, ele, left, right, mid, flag=0; 
    int arr[10]= {20, 33, 45, 67, 89, 99, 120, 133, 150, 170};
    printf("\nEnter an element to be searched: ");
    scanf("%d", &ele);
    left=0, right=9; 
    
    while(left<=right)
    {
        mid=(left+right)/2;
    if(ele>arr[mid])
    {
       left=mid+1; 
    }
    else if(ele<arr[mid])
    {
        right=mid-1;
    }
    else if(arr[mid]==ele)
    {
        printf("\nThe element is present in the array!");
        flag++;
        break;
    }
    }
    if(flag==0)
    {
        printf("\nElement not found in the array.");
    }

    return 0;
}
